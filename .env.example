# Rootstock RPC URLs
RSK_TESTNET_RPC=https://public-node.testnet.rsk.co
RSK_MAINNET_RPC=https://public-node.rsk.co

# Private key for deployment (DO NOT COMMIT!)
# Generate a new wallet for deployment and add the private key here
PRIVATE_KEY=your_private_key_here

# Optional: Rootstock Explorer API key for contract verification
RSK_EXPLORER_API_KEY=your_api_key_here

# Optional: CoinMarketCap API key for gas reporting
COINMARKETCAP_API_KEY=your_api_key_here

# Optional: Enable gas reporting
REPORT_GAS=true
